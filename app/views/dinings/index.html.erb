<h1>Listing Dinings</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Food</th>
      <th>Content</th>
      <th>Location</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dinings.each do |dining| %>
      <tr>
        <td><%= image_tag dining.image.url(:medium) %></td>
        <td><%= dining.name %></td>
        <td><%= dining.food %></td>
        <td><%= dining.content %></td>
        <td><%= dining.location %></td>
        <td><%= dining.phone %></td>
        <td><%= link_to 'Show', dining %></td>
        <td><%= link_to 'Edit', edit_dining_path(dining) %></td>
        <td><%= link_to 'Destroy', dining, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>



